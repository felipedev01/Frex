{
  "expo": {
    "name": "Frex",
    "slug": "frex",
    "icon": "./src/assets/icon.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#9333EA"
    },
    "android": {
      "package": "com.felipecs88.frex",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.felipecs88.frex",
      "infoPlist": {
        "NSCameraUsageDescription": "O app precisa de acesso à câmera para tirar fotos dos comprovantes."
      }
    },
    "plugins": [
      "expo-image-picker"
    ],
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "5bafd9bb-5613-4303-af1f-a4bc31f9b4f4"
      }
    }
  }
}
