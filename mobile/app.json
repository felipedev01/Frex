{
  "expo": {
    "name": "FreX",
    "slug": "frex",
    "icon": "./src/assets/icon.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.felipecs88.frex",
      "permissions": ["CAMERA"]
    },
    "ios": {
      "bundleIdentifier": "com.felipecs88.frex",
      "infoPlist": {
        "NSCameraUsageDescription": "O app precisa de acesso à câmera para tirar fotos dos comprovantes."
      }
    },
    "plugins": [
      "expo-image-picker"
    ],
    "newArchEnabled": true
  }
}